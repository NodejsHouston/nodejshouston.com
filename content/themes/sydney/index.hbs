{{!< default}}

<div id="hero">
  <div class="container">
    <div class="row">
      <p>Node.js Houston was created to bring like minded software developers with an interest of Node.js together. We are all looking forward to the future of web development with a full javascript stack!</p>
    </div>
  </div>
</div>
  
<main>
  {{#foreach posts}}
  <article>
    <header>
      <h1><a href="{{url}}">{{{title}}}</a></h1>
      <h5><time datetime="{{date format='YYYY-MM-DD'}}">{{date format="dddd, MMMM DD, YYYY"}}</time></h5>
    </header>
    <section class="posts">
      <p>{{excerpt}}&hellip;</p>
      <h5><a href="{{url}}">Read more <i class="fa fa-caret-right"></i></a></h5>
    </section>
<!-- Uncomment this section (and comment out the section with {{excerpt}} ) if you want syntax highlighting on your index page.
    <section class="posts">
      {{content}}
    </section>
-->
  </article>
  {{/foreach}}
  {{pagination}}
</main>
